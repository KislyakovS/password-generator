{
  "clearMocks": true,
  "preset": "ts-jest/presets/default-esm",
  "globals": {
    "ts-jest": {
      "useESM": true
    }
  },
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.ts$": "$1",
    "\\.(css|scss)$": "<rootDir>/__mocks__/style.ts",
    "\\.svg": "<rootDir>/__mocks__/icon.ts"
  }
}